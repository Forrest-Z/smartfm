<launch>
    <node pkg="feature_detection" type="cluster_filter.py" name="cluster_filter" output="screen" args="'width &lt; 3.0 and depth &lt; 1.5 and dist &lt; 20'">
        <remap from="cluster_filter_in" to="pedestrian_clusters"/>
        <remap from="cluster_filter_out" to="pedestrian_clusters_filtered"/>
    </node>
    <node pkg="feature_detection" type="simple_detection" name="simple_detection" output="screen">
        <remap from="input_scan" to="/Laser/sick_laser"/>
        <param name="svg_file" value="$(find utown_tjunction)/detection_boundary.svg"/>
    </node>
    <include file="$(find utown_tjunction)/tf.launch"/>
</launch>
