<launch>
        <node pkg="uvc_cam" type="uvc_cam_node" name="camera_front" output="screen" ns="robot_0">
        <remap from="camera" to="camera_front"/>
        <param name="width" type="int" value="1280" />
        <param name="height" type="int" value="720" />
        <param name="frame_rate" type="int" value="20" />
        <param name="frame_id" type="string" value="robot_0/camera_image"/>
        <param name="camera_name" type="string" value="robot_0/camera_front"/>
	<param name="device" type="string" value="/dev/video1"/>
	<param name="camera_info_url" type="string" value="file://$(find uvc_cam)/logitech_webcam_param_camera0.ini" />
        <param name="format_mode" value="2"/>
        <param name="exposure" value="1"/>
        <param name="brightness" value="150"/>
        <param name="contrast" value="50"/>
        <param name="sharpness" value="250"/>
    </node>
</launch>
