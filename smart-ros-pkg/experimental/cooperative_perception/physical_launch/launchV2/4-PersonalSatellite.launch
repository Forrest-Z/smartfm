<launch>
	<!--for the ego vehicle: add "/golfcart"-->
	<node pkg="tf" type="static_transform_publisher" name="base_link_to_camera_front_baseH" args="1.75, 0.0, 30.0 -0.0 1.15 -0.0 /golfcart/base_link /golfcart/cameraH_base 100" />
	<node pkg="tf" type="static_transform_publisher" name="camera_front_base_to_imgH" args="0.0, 0.0, 0.0 -1.5707963267949 0.0 -1.5707963267949 /golfcart/cameraH_base /golfcart/cameraH_front_img 100" />
	<!--node pkg="tf" type="static_transform_publisher" name="base_link_to_camera_front_baseH" args="1.75, 0.0, 7.0 -0.0 0.45 -0.0 /golfcart/base_link /golfcart/cameraH_base 100" />
	<node pkg="tf" type="static_transform_publisher" name="camera_front_base_to_imgH" args="0.0, 0.0, 0.0 -1.5707963267949 0.0 -1.5707963267949 /golfcart/cameraH_base /golfcart/cameraH_front_img 100" />
