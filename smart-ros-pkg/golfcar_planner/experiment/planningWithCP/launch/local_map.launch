<launch>
	<node name="local_map" pkg="local_map" type="local_map_node" output="screen">
		<remap from="scan_in" to="/sickldmrs/scan3"/>
		<remap from="scan_in2" to="/robot_1/scan"/>
		<remap from="scan_in3" to="/robot_2/scan"/>
		<remap from="static_map" to="static_map_lane"/>
	</node>
	<node name="map_server_lane" pkg="map_server_ext" type="map_server" args="$(find planningWithCP)/utown.yaml">
		<remap from="static_map" to="static_map_lane"/>
		<remap from="map" to="lane_map"/>
	</node>
	
	<node name ="obst_detect_test" pkg = "obstacle_tracking" type = "obst_detect">
		<remap from = "base_scan" to="/sickldmrs/scan3"/>
		<remap from="scan_in2" to="/robot_1/scan"/>
		<remap from="scan_in3" to="/robot_2/scan"/>
	</node>
	
	<node name="simple_goal" pkg="rrts_exp" type="simple_goal"/>
	
	<include file="$(find local_map)/urdf/display.launch"/>	
	
</launch>
