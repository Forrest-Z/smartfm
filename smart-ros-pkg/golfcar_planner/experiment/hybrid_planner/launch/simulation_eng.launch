<launch>

    <param name="use_sim_time" value="true"/>

    <node name="fake_localization" pkg="fake_localization" type="fake_localization" respawn="false" />
	
	<!--Start of local_map for rrts planner-->
	<node name="local_map" pkg="local_map_ext" type="local_map_node" output="screen">
		<remap from="scan_in" to="sick_scan2"/>
		<remap from="static_map" to="static_map_lane"/>
	</node>
	
	<node name="map_server_lane" pkg="map_server_ext" type="map_server" args="$(find local_map_ext)/lane_map.yaml">
		<remap from="static_map" to="static_map_lane"/>
		<remap from="map" to="lane_map"/>
	</node>
	
	<node name="simple_goal" pkg="rrts" type="simple_goal" />
	
	<!--End of local_map for rrts planner-->

    <node pkg="stage" type="stageros" name="stageros" args="$(find hybrid_amcl)/worlds/golfcar_eng_sde.world" respawn="false">
        <param name="base_watchdog_timeout" value="0.2"/>
        <remap from="base_scan" to="sick_scan2"/>
    </node>

    <node name="plane_map_server" pkg="map_server" type="map_server" args="$(find hybrid_amcl)/worlds/bigeng_sde_map.yaml">
        <remap from="map" to="curb_map"/>
    </node>

    <include file="$(find hybrid_planner)/launch/move_base.launch"/>

    <node pkg="rviz" type="rviz" name="rviz" respawn="false" args="-d $(find Launch)/nav_simulation_planner.vcg" />
    
    <include file="$(find local_map_ext)/urdf/display.launch"/>

</launch>

