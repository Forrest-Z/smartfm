<launch>
	 <node name="ped_momdp" pkg="ped_momdp_sarsop" type="ped-momdp-sarsop" respawn="false" output="screen">
	    <param name="model_file" value="$(find ped_momdp_sarsop)/policy/rss-lane-u2.pomdpx"/>
	    <param name="policy_file" value="$(find ped_momdp_sarsop)/policy/rss-lane-momdp-u1.policy"/>
	    <param name="simLen" value="100"/>
	    <param name="simNum" value="100"/>
	    <remap from="robot_0/amcl_pose" to="amcl_pose"/>
	    <remap from="robot_0/cmd_vel" to="momdp_vel"/>
            <remap from="odom" to="encoder_odom"/>
	 </node>
	<node name="transform_ped_to_map" type="TransformPedtoMap" pkg="dummyPed" output="screen">
		<param name="ped_id" type="str" value="772, 792, 776"/>
	</node>
    <node pkg="golfcar_ppc" type="speed_advisor" name="speed_advisor" output="screen">
        <rosparam file="$(find Launch)/golfcar/speed_advisor.yaml" command="load"/>
    </node>
</launch>
