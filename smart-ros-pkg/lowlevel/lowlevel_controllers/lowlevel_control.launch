<launch>
    <node name="speed_controller" pkg="lowlevel" type="speed_controller" output="screen">
        <param name="kp" value="0.3"/>
        <param name="ki" value="0.2"/>
        <param name="kd" value="0.1"/>
        <param name="ctrl_period" value="0.01"/>
    </node>
    <node name="vel_cmd" pkg="rostopic" type="rostopic" args="pub cmd_vel geometry_msgs/Twist -r 10 '[1.5,0,0]' '[0,0,0]'" output="screen"/>
    <node name="rxplot" pkg="rxtools" type="rxplot" args="-p 30 /encoders/v,/cmd_vel/linear/x,/pid_gain/v_filter /pid_gain/p_gain,/pid_gain/i_gain,/pid_gain/d_gain,/pid_gain/prior,/pid_gain/u_ctrl" output="screen"/>
    <node name="bag" pkg="rosbag" type="record" args="-a" output="screen"/>
</launch>
