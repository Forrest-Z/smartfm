<launch>
  <param name="use_sim_time" value="true"/>
  <node name="local_map" pkg="local_map_ext" type="local_map_node">
    <remap from="scan_in" to="base_scan"/>
  </node>
  <node name="map_server" pkg="map_server_ext" type="map_server" args="$(find local_map_ext)/dense+curb+lane.yaml"/>
  <node name="rviz" pkg="rviz" type="rviz" args="-d $(find local_map_ext)/local_map_vis.vcg"/>
  <node name="stageros" pkg="stage" type="stageros" args="$(find local_map_ext)/golfcar_eng.world"/>
  <node name="fake_localization" pkg="fake_localization" type="fake_localization"/>
</launch>
