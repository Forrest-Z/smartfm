<launch>
     <param name="use_sim_time" value="true"/>

    <node name="fake_localization" pkg="fake_localization" type="fake_localization" respawn="false" />

    <node pkg="stage" type="stageros" name="stageros" args="$(find golfcar_traj_gen)/blank.world" respawn="false">
        <param name="base_watchdog_timeout" value="0.2"/>
        <remap from="base_scan" to="scan"/>
    </node>
   <node pkg="rviz" type="rviz" name="rviz" respawn="false" args="-d $(find golfcar_traj_gen)/pp_simulation.vcg"/>
</launch>
